{
  "name": "auth-jwt-react-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack -p --env.prod",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prestart": "npm run build",
    "start": "http-server dist",
    "dev": "webpack-dev-server --env.dev",
    "debug": "node --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod"
  },
  "author": "John Tasto",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.2.0",
    "jest-cli": "^19.0.2",
    "lodash": "^4.17.4",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "redux-immutable-state-invariant": "^1.2.4",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-server": "^2.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js?(x)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|svg|ico)$": "<rootDir>/tools/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
